#include <iostream>
#include <cmath>
 
int main()
{
    const float A = 4.1, B = 2.7;
    float arr[]{ 1.9, 2.15, 2.34, 2.74, 3.16 };
    float y;
    
    for (float x : arr){
        y = ( A*A*A*pow(x, 0.5) - B * (log(x)/log(5)) ) / (pow(log10(x-1), 3));
        std::cout << "f(" << x << ")= " << y << std::endl;
    }
    
    return 0;
}